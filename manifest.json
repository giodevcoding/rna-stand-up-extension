{
  "manifest_version": 3,
  "name": "Ramsey Network App Stand-Up",
  "description": "Base Level Extension",
  "version": "0.1",
  "minimum_chrome_version": "102",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "rna.png"
  },
  "permissions": ["storage", "alarms", "notifications"],
  "host_permissions": ["https://gitlab.com/ramsey-solutions/b2c/*"],
  "omnibox": {
    "keyword": "api"
  },
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  }
}
